{
  "name": "@chykalophia/clickup-mcp-shared",
  "version": "1.0.0",
  "description": "Shared utilities, types, and schemas for ClickUp MCP Server packages",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node --esm src/index.ts",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "clean": "rm -rf build coverage test-results .jest-cache",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nsxdavid/clickup-mcp-server.git",
    "directory": "packages/shared"
  },
  "keywords": [
    "clickup",
    "mcp",
    "shared",
    "utilities",
    "types",
    "schemas"
  ],
  "author": "David Whatley (https://davidwhatley.com)",
  "contributors": [
    "ClickUp MCP Server - Enhanced Contributors"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nsxdavid/clickup-mcp-server/issues"
  },
  "homepage": "https://github.com/nsxdavid/clickup-mcp-server#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "type": "module",
  "dependencies": {
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^18.19.26",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "files": [
    "build/**/*",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  }
}
