/**
 * Convert markdown to plain text by stripping formatting
 * @param markdown The markdown content to convert
 * @returns Plain text string
 */
function markdownToPlainText(markdown) {
  /* istanbul ignore next */
  cov_15os8cs0qs().f[6]++;
  cov_15os8cs0qs().s[29]++;
  if (
  /* istanbul ignore next */
  (cov_15os8cs0qs().b[9][0]++, !markdown) ||
  /* istanbul ignore next */
  (cov_15os8cs0qs().b[9][1]++, typeof markdown !== 'string')) {
    /* istanbul ignore next */
    cov_15os8cs0qs().b[8][0]++;
    cov_15os8cs0qs().s[30]++;
    return '';
  } else
  /* istanbul ignore next */
  {
    cov_15os8cs0qs().b[8][1]++;
  }
  cov_15os8cs0qs().s[31]++;
  try {
    // First convert to HTML, then strip tags
    const htmlResult =
    /* istanbul ignore next */
    (cov_15os8cs0qs().s[32]++, marked_1.marked.parse(markdown));
    const html =
    /* istanbul ignore next */
    (cov_15os8cs0qs().s[33]++, typeof htmlResult === 'string' ?
    /* istanbul ignore next */
    (cov_15os8cs0qs().b[10][0]++, htmlResult) :
    /* istanbul ignore next */
    (cov_15os8cs0qs().b[10][1]++, ''));
    /* istanbul ignore next */
    cov_15os8cs0qs().s[34]++;
    return html.replace(/<[^>]*>/g, '').replace(/\n\s*\n/g, '\n').trim();
  } catch (error) {
    /* istanbul ignore next */
    cov_15os8cs0qs().s[35]++;
    console.warn('Failed to convert markdown to plain text:', error);
    // Fallback: basic markdown stripping
    /* istanbul ignore next */
    cov_15os8cs0qs().s[36]++;
    return markdown.replace(/#{1,6}\s+/g, '') // Remove headers
    .replace(/\*\*(.*?)\*\*/g, '$1') // Remove bold
    .replace(/\*(.*?)\*/g, '$1') // Remove italic
    .replace(/`(.*?)`/g, '$1') // Remove inline code
    .replace(/```[\s\S]*?```/g, '') // Remove code blocks
    .replace(/\[([^\]]+)\]\([^)]+\)/g, '$1') // Convert links to text
    .replace(/!\[([^\]]*)\]\([^)]+\)/g, '$1') // Convert images to alt text
    .trim();
  }
}
/* istanbul ignore next */
cov_15os8cs0qs().s[37]++;
exports.markdownToPlainText = markdownToPlainText;
/**
 * Detect if content contains markdown formatting
 * @param content The content to check
 * @returns True if content appears to contain markdown
 */
function isMarkdown(content) {
  /* istanbul ignore next */
  cov_15os8cs0qs().f[7]++;
  cov_15os8cs0qs().s[38]++;
  if (
  /* istanbul ignore next */
  (cov_15os8cs0qs().b[12][0]++, !content) ||
  /* istanbul ignore next */
  (cov_15os8cs0qs().b[12][1]++, typeof content !== 'string')) {
    /* istanbul ignore next */
    cov_15os8cs0qs().b[11][0]++;
    cov_15os8cs0qs().s[39]++;
    return false;
  } else
  /* istanbul ignore next */
  {
    cov_15os8cs0qs().b[11][1]++;
  }
  // Check for common markdown patterns
  const markdownPatterns =
  /* istanbul ignore next */
  (cov_15os8cs0qs().s[40]++, [/#{1,6}\s+/,
  // Headers
  /\*\*.*?\*\*/,
  // Bold
  /\*.*?\*/,
  // Italic
  /`.*?`/,
  // Inline code
  /```[\s\S]*?```/,
  // Code blocks
  /\[.*?\]\(.*?\)/,
  // Links
  /!\[.*?\]\(.*?\)/,
  // Images
  /^\s*[-*+]\s+/m,
  // Unordered lists
  /^\s*\d+\.\s+/m,
  // Ordered lists
  /^\s*>\s+/m,
  // Blockquotes
  /~~.*?~~/,
  // Strikethrough
  /==[^=]+==/ // Highlight
  ]);
  /* istanbul ignore next */
  cov_15os8cs0qs().s[41]++;
  return markdownPatterns.some(pattern => {
    /* istanbul ignore next */
    cov_15os8cs0qs().f[8]++;
    cov_15os8cs0qs().s[42]++;
    return pattern.test(content);
  });
}
/* istanbul ignore next */
cov_15os8cs0qs().s[43]++;
exports.isMarkdown = isMarkdown;
/**
 * Detect if content contains HTML formatting
 * @param content The content to check
 * @returns True if content appears to contain HTML
 */
function isHtml(content) {
  /* istanbul ignore next */
  cov_15os8cs0qs().f[9]++;
  cov_15os8cs0qs().s[44]++;
  if (
  /* istanbul ignore next */
  (cov_15os8cs0qs().b[14][0]++, !content) ||
  /* istanbul ignore next */
  (cov_15os8cs0qs().b[14][1]++, typeof content !== 'string')) {
    /* istanbul ignore next */
    cov_15os8cs0qs().b[13][0]++;
    cov_15os8cs0qs().s[45]++;
    return false;
  } else
  /* istanbul ignore next */
  {
    cov_15os8cs0qs().b[13][1]++;
  }
  // Check for HTML tags
  cov_15os8cs0qs().s[46]++;
  return /<[^>]+>/g.test(content);
}
/* istanbul ignore next */
cov_15os8cs0qs().s[47]++;
