/**
 * Smart content formatter that detects format and converts appropriately
 * @param content The content to format
 * @param targetFormat The desired output format
 * @returns Formatted content
 */
function formatContent(content, targetFormat) {
  /* istanbul ignore next */
  cov_15os8cs0qs().f[10]++;
  cov_15os8cs0qs().s[48]++;
  if (
  /* istanbul ignore next */
  (cov_15os8cs0qs().b[16][0]++, !content) ||
  /* istanbul ignore next */
  (cov_15os8cs0qs().b[16][1]++, typeof content !== 'string')) {
    /* istanbul ignore next */
    cov_15os8cs0qs().b[15][0]++;
    cov_15os8cs0qs().s[49]++;
    return '';
  } else
  /* istanbul ignore next */
  {
    cov_15os8cs0qs().b[15][1]++;
  }
  // Detect current format
  const isCurrentlyHtml =
  /* istanbul ignore next */
  (cov_15os8cs0qs().s[50]++, isHtml(content));
  const isCurrentlyMarkdown =
  /* istanbul ignore next */
  (cov_15os8cs0qs().s[51]++,
  /* istanbul ignore next */
  (cov_15os8cs0qs().b[17][0]++, !isCurrentlyHtml) &&
  /* istanbul ignore next */
  (cov_15os8cs0qs().b[17][1]++, isMarkdown(content)));
  /* istanbul ignore next */
  cov_15os8cs0qs().s[52]++;
  switch (targetFormat) {
    case 'html':
      /* istanbul ignore next */
      cov_15os8cs0qs().b[18][0]++;
      cov_15os8cs0qs().s[53]++;
      if (isCurrentlyHtml) {
        /* istanbul ignore next */
        cov_15os8cs0qs().b[19][0]++;
        cov_15os8cs0qs().s[54]++;
        return content;
      } else
      /* istanbul ignore next */
      {
        cov_15os8cs0qs().b[19][1]++;
      }
      cov_15os8cs0qs().s[55]++;
      if (isCurrentlyMarkdown) {
        /* istanbul ignore next */
        cov_15os8cs0qs().b[20][0]++;
        cov_15os8cs0qs().s[56]++;
        return markdownToHtml(content);
      } else
      /* istanbul ignore next */
      {
        cov_15os8cs0qs().b[20][1]++;
      }
      cov_15os8cs0qs().s[57]++;
      return content;
    // Plain text, return as-is
    case 'markdown':
      /* istanbul ignore next */
      cov_15os8cs0qs().b[18][1]++;
      cov_15os8cs0qs().s[58]++;
      if (isCurrentlyMarkdown) {
        /* istanbul ignore next */
        cov_15os8cs0qs().b[21][0]++;
        cov_15os8cs0qs().s[59]++;
        return content;
      } else
      /* istanbul ignore next */
      {
        cov_15os8cs0qs().b[21][1]++;
      }
      cov_15os8cs0qs().s[60]++;
      if (isCurrentlyHtml) {
        /* istanbul ignore next */
        cov_15os8cs0qs().b[22][0]++;
        cov_15os8cs0qs().s[61]++;
        return htmlToMarkdown(content);
      } else
      /* istanbul ignore next */
      {
        cov_15os8cs0qs().b[22][1]++;
      }
      cov_15os8cs0qs().s[62]++;
      return content;
    // Plain text, return as-is
    case 'plain':
      /* istanbul ignore next */
      cov_15os8cs0qs().b[18][2]++;
      cov_15os8cs0qs().s[63]++;
      if (isCurrentlyMarkdown) {
        /* istanbul ignore next */
        cov_15os8cs0qs().b[23][0]++;
        cov_15os8cs0qs().s[64]++;
        return markdownToPlainText(content);
      } else
      /* istanbul ignore next */
      {
        cov_15os8cs0qs().b[23][1]++;
      }
      cov_15os8cs0qs().s[65]++;
      if (isCurrentlyHtml) {
        /* istanbul ignore next */
        cov_15os8cs0qs().b[24][0]++;
        cov_15os8cs0qs().s[66]++;
        return htmlToMarkdown(content).replace(/[*_`#\[\]()]/g, '');
      } else
      /* istanbul ignore next */
      {
        cov_15os8cs0qs().b[24][1]++;
      }
      cov_15os8cs0qs().s[67]++;
      return content;
    // Already plain text
    default:
      /* istanbul ignore next */
      cov_15os8cs0qs().b[18][3]++;
      cov_15os8cs0qs().s[68]++;
      return content;
  }
}
