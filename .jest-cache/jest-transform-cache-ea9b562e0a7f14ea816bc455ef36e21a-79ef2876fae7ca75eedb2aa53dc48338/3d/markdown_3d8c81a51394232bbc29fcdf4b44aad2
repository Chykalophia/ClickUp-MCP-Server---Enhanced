/**
 * Convert HTML to markdown for display/editing
 * @param html The HTML content to convert
 * @returns Markdown string
 */
function htmlToMarkdown(html) {
  /* istanbul ignore next */
  cov_15os8cs0qs().f[5]++;
  cov_15os8cs0qs().s[22]++;
  if (
  /* istanbul ignore next */
  (cov_15os8cs0qs().b[7][0]++, !html) ||
  /* istanbul ignore next */
  (cov_15os8cs0qs().b[7][1]++, typeof html !== 'string')) {
    /* istanbul ignore next */
    cov_15os8cs0qs().b[6][0]++;
    cov_15os8cs0qs().s[23]++;
    return '';
  } else
  /* istanbul ignore next */
  {
    cov_15os8cs0qs().b[6][1]++;
  }
  cov_15os8cs0qs().s[24]++;
  try {
    /* istanbul ignore next */
    cov_15os8cs0qs().s[25]++;
    return turndownService.turndown(html);
  } catch (error) {
    /* istanbul ignore next */
    cov_15os8cs0qs().s[26]++;
    console.warn('Failed to convert HTML to markdown, returning as plain text:', error);
    // Strip HTML tags as fallback
    /* istanbul ignore next */
    cov_15os8cs0qs().s[27]++;
    return html.replace(/<[^>]*>/g, '');
  }
}
