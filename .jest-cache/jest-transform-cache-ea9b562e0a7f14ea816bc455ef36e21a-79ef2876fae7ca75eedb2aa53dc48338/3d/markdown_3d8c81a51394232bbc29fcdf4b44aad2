/**
 * Prepare content for ClickUp API submission
 * Converts markdown to HTML and provides both formats
 * @param content The content to prepare (markdown or plain text)
 * @returns Object with both HTML and plain text versions
 */
function prepareContentForClickUp(content) {
  /* istanbul ignore next */
  cov_15os8cs0qs().f[11]++;
  cov_15os8cs0qs().s[70]++;
  if (
  /* istanbul ignore next */
  (cov_15os8cs0qs().b[26][0]++, !content) ||
  /* istanbul ignore next */
  (cov_15os8cs0qs().b[26][1]++, typeof content !== 'string')) {
    /* istanbul ignore next */
    cov_15os8cs0qs().b[25][0]++;
    cov_15os8cs0qs().s[71]++;
    return {
      description: ''
    };
  } else
  /* istanbul ignore next */
  {
    cov_15os8cs0qs().b[25][1]++;
  }
  // If content looks like markdown, convert to HTML
  cov_15os8cs0qs().s[72]++;
  if (isMarkdown(content)) {
    /* istanbul ignore next */
    cov_15os8cs0qs().b[27][0]++;
    const html =
    /* istanbul ignore next */
    (cov_15os8cs0qs().s[73]++, markdownToHtml(content));
    const plainText =
    /* istanbul ignore next */
    (cov_15os8cs0qs().s[74]++, markdownToPlainText(content));
    /* istanbul ignore next */
    cov_15os8cs0qs().s[75]++;
    return {
      description: html,
      text_content: plainText
    };
  } else
  /* istanbul ignore next */
  {
    cov_15os8cs0qs().b[27][1]++;
  }
  // If content is already HTML, use as-is
  cov_15os8cs0qs().s[76]++;
  if (isHtml(content)) {
    /* istanbul ignore next */
    cov_15os8cs0qs().b[28][0]++;
    const plainText =
    /* istanbul ignore next */
    (cov_15os8cs0qs().s[77]++, htmlToMarkdown(content));
    /* istanbul ignore next */
    cov_15os8cs0qs().s[78]++;
    return {
      description: content,
      text_content: markdownToPlainText(plainText)
    };
  } else
  /* istanbul ignore next */
  {
    cov_15os8cs0qs().b[28][1]++;
  }
  // Plain text content
  cov_15os8cs0qs().s[79]++;
  return {
    description: content,
    text_content: content
  };
}
/* istanbul ignore next */
cov_15os8cs0qs().s[80]++;
exports.prepareContentForClickUp = prepareContentForClickUp;
/**
 * Process ClickUp response content for display
 * Converts HTML to markdown for better readability
 * @param response ClickUp API response with description/text_content
 * @returns Processed content with markdown formatting
 */
function processClickUpResponse(response) {
  /* istanbul ignore next */
  cov_15os8cs0qs().f[12]++;
  cov_15os8cs0qs().s[81]++;
  if (
  /* istanbul ignore next */
  (cov_15os8cs0qs().b[30][0]++, !response) ||
  /* istanbul ignore next */
  (cov_15os8cs0qs().b[30][1]++, typeof response !== 'object')) {
    /* istanbul ignore next */
    cov_15os8cs0qs().b[29][0]++;
    cov_15os8cs0qs().s[82]++;
    return response;
  } else
  /* istanbul ignore next */
  {
    cov_15os8cs0qs().b[29][1]++;
  }
  const processed =
  /* istanbul ignore next */
  (cov_15os8cs0qs().s[83]++, {
    ...response
  });
  // Process description field
  /* istanbul ignore next */
  cov_15os8cs0qs().s[84]++;
  if (
  /* istanbul ignore next */
  (cov_15os8cs0qs().b[32][0]++, processed.description) &&
  /* istanbul ignore next */
  (cov_15os8cs0qs().b[32][1]++, isHtml(processed.description))) {
    /* istanbul ignore next */
    cov_15os8cs0qs().b[31][0]++;
    cov_15os8cs0qs().s[85]++;
    processed.description_markdown = htmlToMarkdown(processed.description);
  } else
  /* istanbul ignore next */
  {
    cov_15os8cs0qs().b[31][1]++;
  }
  // Process comment fields
  cov_15os8cs0qs().s[86]++;
  if (
  /* istanbul ignore next */
  (cov_15os8cs0qs().b[34][0]++, processed.comment_text) &&
  /* istanbul ignore next */
  (cov_15os8cs0qs().b[34][1]++, processed.comment) &&
  /* istanbul ignore next */
  (cov_15os8cs0qs().b[34][2]++, Array.isArray(processed.comment))) {
    /* istanbul ignore next */
    cov_15os8cs0qs().b[33][0]++;
    cov_15os8cs0qs().s[87]++;
    // ClickUp comments come as rich text blocks, try to convert to markdown
    try {
      const htmlContent =
      /* istanbul ignore next */
      (cov_15os8cs0qs().s[88]++, processed.comment.map(block => {
        /* istanbul ignore next */
        cov_15os8cs0qs().f[13]++;
        cov_15os8cs0qs().s[89]++;
        if (typeof block === 'string') {
          /* istanbul ignore next */
          cov_15os8cs0qs().b[35][0]++;
          cov_15os8cs0qs().s[90]++;
          return block;
        } else
        /* istanbul ignore next */
        {
          cov_15os8cs0qs().b[35][1]++;
        }
        cov_15os8cs0qs().s[91]++;
        if (block.text) {
          /* istanbul ignore next */
          cov_15os8cs0qs().b[36][0]++;
          cov_15os8cs0qs().s[92]++;
          return block.text;
        } else
        /* istanbul ignore next */
        {
          cov_15os8cs0qs().b[36][1]++;
        }
        cov_15os8cs0qs().s[93]++;
        return '';
      }).join(''));
      /* istanbul ignore next */
      cov_15os8cs0qs().s[94]++;
      if (
      /* istanbul ignore next */
      (cov_15os8cs0qs().b[38][0]++, htmlContent) &&
      /* istanbul ignore next */
      (cov_15os8cs0qs().b[38][1]++, isHtml(htmlContent))) {
        /* istanbul ignore next */
        cov_15os8cs0qs().b[37][0]++;
        cov_15os8cs0qs().s[95]++;
        processed.comment_markdown = htmlToMarkdown(htmlContent);
      } else
      /* istanbul ignore next */
      {
        cov_15os8cs0qs().b[37][1]++;
      }
    } catch (error) {
      /* istanbul ignore next */
      cov_15os8cs0qs().s[96]++;
      console.warn('Failed to process comment blocks:', error);
    }
  } else
  /* istanbul ignore next */
  {
    cov_15os8cs0qs().b[33][1]++;
  }
  cov_15os8cs0qs().s[97]++;
  return processed;
}
/* istanbul ignore next */
cov_15os8cs0qs().s[98]++;
exports.processClickUpResponse = processClickUpResponse;
