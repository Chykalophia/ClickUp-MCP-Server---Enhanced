{
  "name": "@chykalophia/clickup-intelligence-mcp-server",
  "version": "1.0.0",
  "description": "Advanced project management intelligence and workflow optimization for ClickUp - AI-powered analytics, smart planning, and automation",
  "main": "build/index.js",
  "bin": {
    "clickup-intelligence-mcp-server": "build/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node --esm src/index.ts",
    "start": "node build/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "clean": "rm -rf build coverage test-results .jest-cache",
    "prepare": "npm run build",
    "prepack": "npm run test:coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nsxdavid/clickup-mcp-server.git",
    "directory": "packages/intelligence"
  },
  "keywords": [
    "clickup",
    "mcp",
    "model-context-protocol",
    "ai",
    "intelligence",
    "analytics",
    "project-management",
    "workflow-optimization",
    "smart-planning",
    "automation",
    "health-analyzer",
    "sprint-planner",
    "task-decomposition",
    "resource-optimizer",
    "workflow-intelligence"
  ],
  "author": "David Whatley (https://davidwhatley.com)",
  "contributors": [
    "ClickUp MCP Server - Enhanced Contributors"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nsxdavid/clickup-mcp-server/issues"
  },
  "homepage": "https://github.com/nsxdavid/clickup-mcp-server#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "type": "module",
  "dependencies": {
    "@chykalophia/clickup-mcp-shared": "^1.0.0",
    "@modelcontextprotocol/sdk": "^1.6.1",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "zod": "^3.22.4"
  },
  "peerDependencies": {
    "@chykalophia/clickup-mcp-server": "^3.4.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^18.19.26",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "files": [
    "build/**/*",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  }
}
