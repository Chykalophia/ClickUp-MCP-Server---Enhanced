/**
 * Convert markdown to HTML for ClickUp API submission
 * @param markdown The markdown content to convert
 * @returns HTML string suitable for ClickUp API
 */
function markdownToHtml(markdown) {
  /* istanbul ignore next */
  cov_15os8cs0qs().f[4]++;
  cov_15os8cs0qs().s[14]++;
  if (
  /* istanbul ignore next */
  (cov_15os8cs0qs().b[4][0]++, !markdown) ||
  /* istanbul ignore next */
  (cov_15os8cs0qs().b[4][1]++, typeof markdown !== 'string')) {
    /* istanbul ignore next */
    cov_15os8cs0qs().b[3][0]++;
    cov_15os8cs0qs().s[15]++;
    return '';
  } else
  /* istanbul ignore next */
  {
    cov_15os8cs0qs().b[3][1]++;
  }
  cov_15os8cs0qs().s[16]++;
  try {
    const result =
    /* istanbul ignore next */
    (cov_15os8cs0qs().s[17]++, marked_1.marked.parse(markdown));
    /* istanbul ignore next */
    cov_15os8cs0qs().s[18]++;
    return typeof result === 'string' ?
    /* istanbul ignore next */
    (cov_15os8cs0qs().b[5][0]++, result) :
    /* istanbul ignore next */
    (cov_15os8cs0qs().b[5][1]++, '');
  } catch (error) {
    /* istanbul ignore next */
    cov_15os8cs0qs().s[19]++;
    console.warn('Failed to parse markdown, returning as plain text:', error);
    /* istanbul ignore next */
    cov_15os8cs0qs().s[20]++;
    return markdown;
  }
}
