{
  "name": "clickup-server",
  "version": "1.5.0",
  "description": "A Model Context Protocol (MCP) server that provides a standardized interface for AI assistants to interact with the ClickUp API",
  "main": "build/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node --esm src/index.ts",
    "start": "node build/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/clickup-server.git"
  },
  "keywords": [
    "clickup",
    "mcp",
    "model-context-protocol",
    "ai",
    "assistant",
    "api",
    "server"
  ],
  "author": "ClickUp MCP Server Contributors",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/your-username/clickup-server/issues"
  },
  "homepage": "https://github.com/your-username/clickup-server#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "type": "module",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.6.1",
    "axios": "^1.8.2",
    "dotenv": "^16.4.7",
    "express": "^5.0.1",
    "open": "^10.1.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  }
}
